            <li class="job-details-item">1d ago</li>
            <li class="job-details-item">Full Time</li>
            <li class="job-details-item">USA only</li>
          </ul>
        </div>
        <hr>
      </div>
      <div class="col-2">
        <div class="attributes">
          <!-- Languages -->
          <ul class="attribute-list">
            <!-- role -->
            <li>Frontend</li>
            <!-- level -->
            <li>Senior</li>
            <!-- languages -->
            <li>HTML</li>
            <li>CSS</li>
            <li>JavaScript</li>
          </ul>
        </div>
      </div>
