        <div class="logo">
          <img src="images/photosnap.svg" alt="Photosnap">
        </div>
        <div class="company-details">
          <div class="company">Photosnap</div>
          <div class="btn new-btn">New!</div>
          <div class="btn featured-btn">Featured</div>
          <h2 class="position">Senior Frontend Developer</h2>
